{
  "name": "mui-status",
  "version": "0.0.42",
  "description": "<MuiStatus> VSCode like status auto-managed component system with MUI5 interface",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "private": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.4.0",
    "copy-and-watch": "^0.1.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "rollup-plugin-visualizer": "^5.8.3",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "npm": "^8.18.0",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "clsx": "^1.2.1",
    "css": "^3.0.0",
    "tss-react": "^4.3.4"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib"
  ],
  "keywords": [
    "reactjs",
    "self managed status",
    "status-bar",
    "status manager",
    "mui-status"
  ],
  "author": "rand0mC0d3r",
  "license": "MIT",
  "scripts": {
    "preclean": "npm ls react | grep \"node_modules\" | grep \"/../\"",
    "clean": "rm -rf node_modules && npm cache clean --force",
    "*TOOLS*": "--------------------------------------------------------------",
    "list": "npm list | grep '\\.\\/\\.\\.'",
    "*COMPONENT*": "------------------------------------------------------------",
    "#precopy.component": "npx eslint 'src/**'",
    "copy.component": "concurrently \"npx eslint 'src/**'\" \"npm run build.lib.esm && du -sh ./lib/esm\"",
    "watch.component": "nodemon -e tsx,ts --watch ./src --exec \"npm run copy.component\"",
    "*DEVELOP*": "--------------------------------------------------------------",
    "start": "npm run link && npm run watch.component",
    "*BUILD*": "----------------------------------------------------------------",
    "prebuild.lib": "rm -rf ./lib && mkdir -p lib",
    "build.lib": "concurrently \"tsc\" \"tsc --module commonjs --outDir lib/cjs\"",
    "build.lib.esm": "tsc",
    "*TEST*": "-----------------------------------------------------------------",
    "lint": "npx eslint --init",
    "test": "react-scripts test",
    "*LINKING*": "--------------------------------------------------------------",
    "#prelink": "npm run list",
    "link": "npm link @mui/material @mui/icons-material react react-dom @types/react",
    "postlink": "npm run list",
    "*RELEASE*": "--------------------------------------------------------------",
    "tag": "rm -rf lib/* && npm run build.lib && git commit -am 'built next version' && npm version patch && git push && git push --tags && npm publish"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rand0mC0d3r/mui-status.git"
  },
  "bugs": {
    "url": "https://github.com/rand0mC0d3r/mui-status/issues"
  },
  "homepage": "https://github.com/rand0mC0d3r/mui-status/blob/master/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
