{
  "name": "mui-status",
  "version": "0.0.18",
  "description": "<MuiStatus> VSCode like status self-managed component system with mui5 interface",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "private": false,
  "config": {
    "helperProjectPath": "../mui-status-helper/node_modules"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "copy-and-watch": "^0.1.6",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "i": "^0.3.7",
    "nodemon": "^2.0.19",
    "npm": "^8.18.0",
    "npm-check-updates": "^16.0.5",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "clsx": "^1.2.1",
    "css": "^3.0.0",
    "prop-types": "^15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib"
  ],
  "keywords": [
    "reactjs",
    "self managed status",
    "status-bar",
    "status manager",
    "mui-status"
  ],
  "author": "rand0mC0d3r",
  "license": "MIT",
  "scripts": {
    "xdev": "npm run copy.component && cd ${npm_package_config_helperProjectPath} && npm run start\"",
    "dev": "npm run watch.component",
    "start": "serve -s build",
    "prebuild": "npm install ./sample && npm run clone.component",
    "prebuild.lib": "rm -rf ./lib && mkdir -p lib",
    "build.lib": "concurrently \"tsc\" \"tsc --module commonjs --outDir lib/cjs\"",
    "postbuild.lib": "npm link",
    "test": "react-scripts test",
    "*COMPONENT*": "###",
    "watch.component": "nodemon -e tsx,ts --watch ./src --exec \"npm run copy.component\"",
    "copy.component": "rm -rf *.tgz && npm run build.lib",
    "*RELEASE*": "###",
    "tag": "rm -rf lib/* && npm run build.lib && git commit -am 'built next version' && npm version patch && git push && git push --tags && npm publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rand0mC0d3r/mui-status.git"
  },
  "bugs": {
    "url": "https://github.com/rand0mC0d3r/mui-status/issues"
  },
  "homepage": "https://github.com/rand0mC0d3r/mui-status/blob/master/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
